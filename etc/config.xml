<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <cssoft_core>
            <notification>
                <enabled>1</enabled>
                <use_https>1</use_https>
                <feed_url>cssoftsolutions.com/notifier</feed_url>
            </notification>
            <modules>
                <use_https>0</use_https>
                <url>cssoft.github.io/packages</url>
            </modules>
            <license>
                <use_https>1</use_https>
                <url>/license/validate</url>
            </license>
        </cssoft_core>
        <subscriptionchecker>
            <ignore>
                <cssoft_core/>
            </ignore>
        </subscriptionchecker>
    </default>
</config>
